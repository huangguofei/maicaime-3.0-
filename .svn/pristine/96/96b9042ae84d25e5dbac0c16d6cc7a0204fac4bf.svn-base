import { checkingAPI } from "apiurl/checking"
export default { /*获取客户列表*/
	getCheckingList(vm) {
		checkingAPI.checkingListAPI({
			startDate:vm.Time.sTime,
			endDate:vm.Time.eTime
		}, (result) => {
			vm.checkingList = result.resultList;
			vm.amount=result.sumAmount;
		});
	}
}
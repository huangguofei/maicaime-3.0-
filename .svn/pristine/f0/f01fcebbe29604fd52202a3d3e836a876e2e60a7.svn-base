import {integralAPI} from "apiurl/user"
 export default{
 		/*获取积分内容*/
 		getIntegral(vm) {
 			integralAPI.userIntegralAPI((result) => {
 				vm.integralData = result;
 				
 			});
 		},/*获取积分记录内容*/
 		getIntegralList(vm) {
 			integralAPI.userIntegralListAPI({walletType:0},(result) => {
 				vm.integralListData = result.resultList;
 			});
 		},
 		
 }

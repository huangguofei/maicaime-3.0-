import { orderAPI } from "../../api/order"
export default {
	/*获取订单列表*/
	getOrderList(vm) {
		var str = JSON.stringify(vm.queryParams);
		var params = JSON.parse(str);
		for(var i in params) {
			if(!params[i])
				delete params[i];
		}
		orderAPI.orderListAPI(params, (result) => {
			vm.tableData = result.resultList;
			vm.queryParams.pageNum = result.currentPage;
			vm.queryParams.pages = result.totalPage;
			vm.queryParams.total = result.totalCount;
		});
	},
	/*获取订单详情*/
	getOrderDetails(vm) {
		orderAPI.orderDetailsAPI({
			orderId: vm.orderId
		}, (result) => {
			vm.orderData = result;

		});
	}
}
import {activityAPI} from "../../api/marketing"
export default{
	/*获取活动列表*/
	getActivityList(vm) {
		var paging = vm.pagings;
		var params = {};
		params.pageNum = paging.current;
		activityAPI.activityListAPI(params, (result) => {
			vm.tableData = result.resultList;
			paging.total = result.totalCount;
		});
	}
}
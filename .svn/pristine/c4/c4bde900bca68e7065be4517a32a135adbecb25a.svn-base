/*主色调*/
@m-c:#39C72B;

/*主背景色*/
@m-back:#F3F3F3;

/*商品标题*/
@p-t-c:#000000;
/*商品价格*/
@p-p-c:#F74C31;
/*商品原价*/
@p-o-c:#999999;

/*搜索区域字体颜色*/
@s-c:#999999;

/*默认边框颜色*/
@d-b-c:#999999;
/*商品边框颜色*/
@p-b-c:#E5E5E5;

/*按钮默认色*/
@btn-color:#1EC551;
/*删除按钮*/
@btn-d-color:#FF5B60;
/*编辑按钮*/
@btn-e-color:#D7D7D7;

/*底部导航菜单颜色*/
@f-font-color:#222222;
@f-active-color:#1EC551;

/*转换rem*/
@baseFontSize: 64;
.pxrem(@name, @px){
    @{name}: (@px / 2 + 2)/ @baseFontSize * 1rem;
}
